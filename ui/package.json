{
  "name": "bcl-ndelius-um-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href /umt/",
    "start:dev": "ng serve --base-href /umt/ --configuration dev",
    "build": "ng build --base-href /umt/",
    "build-prod": "ng build --prod --base-href /umt/",
    "watch": "ng build --base-href /umt/ --watch",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "accessibility": "start-server-and-test start:dev 4200/umt 'concurrently npm:accessibility:*'",
    "accessibility:test": "pa11y-ci",
    "accessibility:report": "pa11y-ci --config .pa11yci-v.js --json > pa11y-ci-results.json; pa11y-ci-reporter-html -d dist/reports/pa11y"
  },
  "private": true,
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 9-11"
  ],
  "dependencies": {
    "@angular/animations": "^9.1.7",
    "@angular/common": "^9.1.7",
    "@angular/compiler": "^9.1.7",
    "@angular/core": "^9.1.7",
    "@angular/forms": "^9.1.7",
    "@angular/platform-browser": "^9.1.7",
    "@angular/platform-browser-dynamic": "^9.1.7",
    "@angular/router": "^9.1.7",
    "angular-oauth2-oidc": "^9.2.1",
    "bootstrap": "^4.4.1",
    "jquery": "^3.5.1",
    "moment": "^2.25.3",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "tslib": "^1.11.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.7",
    "@angular/language-service": "^9.1.7",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-junit-reporter": "^2.0.1",
    "pa11y-ci": "^2.3.0",
    "pa11y-ci-reporter-html": "^2.0.2",
    "puppeteer": "^2.1.1",
    "start-server-and-test": "^1.11.0",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
