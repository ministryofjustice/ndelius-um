{
  "name": "bcl-ndelius-um-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --base-href /umt/",
    "start:dev": "ng serve --base-href /umt/ --configuration dev",
    "build": "ng build --base-href /umt/",
    "build-prod": "ng build --prod --base-href /umt/",
    "watch": "ng build --base-href /umt/ --watch",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "accessibility": "wait-port 8080; concurrently npm:accessibility:*",
    "accessibility:test": "pa11y-ci",
    "accessibility:report": "pa11y-ci --config .pa11yci-v.js --json > pa11y-ci-results.json; pa11y-ci-reporter-html -d dist/reports/pa11y"
  },
  "private": true,
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 9-11"
  ],
  "dependencies": {
    "@angular/animations": "v10-lts",
    "@angular/common": "v10-lts",
    "@angular/compiler": "v10-lts",
    "@angular/core": "v10-lts",
    "@angular/forms": "v10-lts",
    "@angular/platform-browser": "v10-lts",
    "@angular/platform-browser-dynamic": "v10-lts",
    "@angular/router": "v10-lts",
    "angular-oauth2-oidc": "^10.0.3",
    "bootstrap": "^4.5.0",
    "file-saver": "^2.0.5",
    "jquery": "^3.5.1",
    "moment": "^2.27.0",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "v10-lts",
    "@angular/cli": "v10-lts",
    "@angular/compiler-cli": "v10-lts",
    "@angular/language-service": "v10-lts",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-htmlfile-reporter": "^0.3.8",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "pa11y-ci": "^2.4.1",
    "pa11y-ci-reporter-html": "^2.1.3",
    "puppeteer": "^2.1.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~4.0.8",
    "wait-port": "^0.2.9"
  }
}
